nst errorField = document.getElementById(\"engraving-error\");\n  const styleDropdown = document.querySelector('[data-engraving-style-select]');\n  const fileUploadWrapper = document.getElementById(\"file-upload-wrapper\");\n\n  function getSelectedEngravingValue() {\n    const radios = document.querySelectorAll('.product-form__input input[type=\"radio\"]');\n    for (let r of radios) {\n      if (r.checked && r.value.includes(\"Custom Engraving\")) {\n        return r.value;\n      }\n    }\n    return \"Standard (Our Logo)\";\n  }\n\n  function toggleEngravingFields() {\n    const selected = getSelectedEngravingValue();\n    const isCustom = selected.includes(\"Custom Engraving\");\n\n    if (isCustom) {\n      engravingWrapper.classList.add(\"active\");\n    } else {\n      engravingWrapper.classList.remove(\"active\");\n      errorField.style.display = \"none\";\n    }\n\n    if (styleDropdown && styleDropdown.value.includes(\"Style 4\")) {\n      fileUploadWrapper.style.display = \"block\";\n    } else {\n      fileUploadWrapper.style.display = \"none\";\n    }\n  }\n\n  // Initial state\n  toggleEngravingFields();\n\n  // Listen for engraving type (radio) changes\n  document.querySelectorAll('.product-form__input input[type=\"radio\"], .product-form__input button').forEach(el => {\n    el.addEventListener(\"click\", function () {\n      setTimeout(toggleEngravingFields, 100);\n    });\n  });\n\n  // Listen for engraving style dropdown changes\n  if (styleDropdown) {\n    styleDropdown.addEventListener(\"change\", toggleEngravingFields);\n  }\n\n  document.addEventListener(\"variant:change\", toggleEngravingFields);\n\n  // Form submission validation\n  const form = document.querySelector('form[action^=\"/cart/add\"]');\n  if (form) {\n    form.noValidate = false;\n    form.addEventListener(\"submit\", function (e) {\n      const value = getSelectedEngravingValue();\n      const isCustom = value.includes(\"Custom Engraving\");\n      const text = engravingTextarea?.value?.trim();\n\n      if (isCustom && (!text || text === \"\")) {\n        e.preventDefault();\n        errorField.style.display = \"block\";\n        engravingTextarea.scrollIntoView({ behavior: \"smooth\", block: \"center\" });\n      } else {\n        errorField.style.display = \"none\";\n      }\n    });\n  }\n});\n</script>"
